general {
	colors = true
	color_good = "#BBBBBB"
	color_bad = "#CC1616"
	color_degraded = "#55858E"
	interval = 2
}

order += "volume master"
order += "tztime local"

volume master {
	device = "default"
	mixer = "Master"
	mixer_idx = 0
  format = " VOL: %volume  " 
}

tztime local {
format = "%d-%m-%y %H:%M:%S"
}
